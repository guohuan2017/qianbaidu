<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="dao.CommercialUserMapper" >

	<insert id="insertSelective" parameterType="pojo.CommercialUser">
		insert into commercial_user(id,username,password,phone) values(#{id},#{username},#{password},#{phone})
	</insert>
	
	<select id="selectBytelPassword" parameterType="pojo.CommercialUser" resultType="pojo.CommercialUser">
		select * from commercial_user where phone = #{phone} and password = #{password}
	</select>
	
	<update id="updateStoreId" parameterType="pojo.CommercialUser">
		update commercial_user set storeid = #{storeidï¼ŒjdbcType=INTEGER} where id = #{id,jdbcType=VARCHAR}
	</update>
</mapper>